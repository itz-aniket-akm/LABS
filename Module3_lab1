        "id": "_lrY1BLhpdzK",
        "outputId": "973974e6-7ea5-43ed-86b4-77d2a8aa3b71"
      },
      "source": [
        "def generate_synth_data(n = 50):\n",
        "    points = np.concatenate((ss.norm(0, 1).rvs((n, 2)), ss.norm(1, 1).rvs((n, 2))), axis = 0)\n",
        "    outcomes = np.concatenate((np.repeat(0, n), np.repeat(1, n)))\n",
        "    return (points, outcomes)\n",
        "\n",
        "n = 50\n",
        "pts,tgts = generate_synth_data(n) #generates 100 points\n",
        "# print(tgts)\n",
        "plt.figure()\n",
        "plt.plot(pts[:n, 0], pts[:n, 1], \"ro\")\n",
        "plt.plot(pts[n:, 0], pts[n:, 1], \"bo\")\n",
        "plt.show()"
      ],
      "execution_count": 26,
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 640x480 with 1 Axes>"
