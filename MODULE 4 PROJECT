        "\n",
        "###task 3\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Make some data\n",
        "x_2 = np.linspace(-2, 2, 1000)\n",
        "\n",
        "# Define the functions\n",
        "def g(x):\n",
        "    return x**3 - 2*x**2 + 2\n",
        "\n",
        "def dg(x):\n",
        "    return 3*x**2 - 4*x  # Derivative of g(x) with respect to x\n",
        "\n",
        "# Create subplots\n",
        "plt.figure(figsize=(12, 4))\n",
        "\n",
        "# Plot the function g(x)\n",
        "plt.subplot(1, 2, 1)\n",
        "plt.plot(x_2, g(x_2), label='g(x)')\n",
        "plt.title('Function: g(x)')\n",
        "plt.xlabel('x')\n",
        "plt.ylabel('g(x)')\n",
        "plt.legend()\n",
        "\n",
        "# Plot the derivative dg(x)\n",
        "plt.subplot(1, 2, 2)\n",
        "plt.plot(x_2, dg(x_2), color='orange', label=\"g'(x)\")\n",
        "plt.title(\"Derivative: g'(x)\")\n",
        "plt.xlabel('x')\n",
        "plt.ylabel(\"g'(x)\")\n",
        "plt.legend()\n",
        "\n",
        "# Adjust layout to prevent overlap\n",
        "plt.tight_layout()\n",
        "\n",
        "# Show the plots\n",
        "plt.show()\n"
