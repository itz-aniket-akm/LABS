      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "U-XBDufnxbuT",
        "outputId": "7974de22-dea8-487e-dab6-1f59fb297e33"
      },
      "execution_count": 32,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "[10, 12]\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "## TASK-1:\n",
        "## In the above case, we saw K-Fold cross validation, where we split the dataset into K consecutive folds without taking anything else into account.\n",
        "## Stratified KFold is another strategy that is commonly used, which preserves the percentage of samples for each class in the folds.\n",
        "## Implement Stratified KFold (refer Sklearn docs) and display the results you obtain\n",
        "from sklearn.model_selection import StratifiedKFold\n",
        "from statistics import mean\n",
        "\n",
        "# Initialize StratifiedKFold\n",
        "stratified_kf = StratifiedKFold(n_splits=10, random_state=17, shuffle=True)\n",
        "\n",
        "# Lists to store results\n",
        "stratified_cv_scores = []\n",
        "stratified_neighbors = []\n",
        "\n",
        "# Loop over different values of k\n",
        "for k in range(1, 25):\n",
        "    knn = KNeighborsClassifier(n_neighbors=k)\n",
        "    stratified_acclist = []\n",
        "\n",
        "    # Loop over folds using StratifiedKFold\n",
        "    for train_index, test_index in stratified_kf.split(X_train, y_train):\n",
        "        x_train_fold, x_test_fold = X_train.iloc[train_index], X_train.iloc[test_index]\n",
        "        y_train_fold, y_test_fold = y_train.iloc[train_index], y_train.iloc[test_index]\n",
        "        knn.fit(x_train_fold, y_train_fold)\n",
        "        stratified_acclist.append(knn.score(x_test_fold, y_test_fold))\n",
        "\n",
        "    # Calculate mean accuracy for the current k\n",
        "    stratified_cv_scores.append(mean(stratified_acclist))\n",
        "    stratified_neighbors.append(k)\n",
        "\n",
        "# Convert accuracy to percentage\n",
        "stratified_Accuracy = [x * 100 for x in stratified_cv_scores]\n",
        "\n",
        "# Plot misclassification error versus k for Stratified KFold\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.plot(stratified_neighbors, stratified_Accuracy)\n",
        "plt.xlabel('Number of neighbors')\n",
        "plt.ylabel('Mean Stratified Cross-validation accuracy in %')\n",
        "plt.show()\n",
        "\n",
        "# Determine the best k for Stratified KFold\n",
        "max_stratified_acc = max(stratified_Accuracy)\n",
        "optimal_stratified_ks = [stratified_neighbors[i] for i, j in enumerate(stratified_Accuracy) if j == max_stratified_acc]\n",
        "print(optimal_stratified_ks)\n",
        "from sklearn.model_selection import StratifiedKFold\n",
        "from statistics import mean\n",
        "\n",
        "# Initialize StratifiedKFold\n",
        "stratified_kf = StratifiedKFold(n_splits=10, random_state=17, shuffle=True)\n",
        "\n",
        "# Lists to store results\n",
        "stratified_cv_scores = []\n",
        "stratified_neighbors = []\n",
        "\n",
        "# Loop over different values of k\n",
        "for k in range(1, 25):\n",
        "    knn = KNeighborsClassifier(n_neighbors=k)\n",
        "    stratified_acclist = []\n",
        "\n",
        "    # Loop over folds using StratifiedKFold\n",
        "    for train_index, test_index in stratified_kf.split(X_train, y_train):\n",
        "        x_train_fold, x_test_fold = X_train.iloc[train_index], X_train.iloc[test_index]\n",
        "        y_train_fold, y_test_fold = y_train.iloc[train_index], y_train.iloc[test_index]\n",
        "        knn.fit(x_train_fold, y_train_fold)\n",
        "        stratified_acclist.append(knn.score(x_test_fold, y_test_fold))\n",
        "\n",
        "    # Calculate mean accuracy for the current k\n",
        "    stratified_cv_scores.append(mean(stratified_acclist))\n",
        "    stratified_neighbors.append(k)\n",
        "\n",
        "# Convert accuracy to percentage\n",
        "stratified_Accuracy = [x * 100 for x in stratified_cv_scores]\n",
        "\n",
        "# Plot misclassification error versus k for Stratified KFold\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.plot(stratified_neighbors, stratified_Accuracy)\n",
        "plt.xlabel('Number of neighbors')\n",
        "plt.ylabel('Mean Stratified Cross-validation accuracy in %')\n",
        "plt.show()\n",
        "\n",
        "# Determine the best k for Stratified KFold\n",
        "max_stratified_acc = max(stratified_Accuracy)\n",
        "optimal_stratified_ks = [stratified_neighbors[i] for i, j in enumerate(stratified_Accuracy) if j == max_stratified_acc]\n",
        "print(optimal_stratified_ks)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 1000
        },
        "outputId": "624b8cf9-3648-45df-f50f-1ca9d43bbd27",
        "id": "L-jhl3DoAXrU"
