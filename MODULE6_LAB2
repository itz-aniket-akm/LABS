    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.datasets import make_regression\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.neighbors import KNeighborsRegressor\n",
        "from math import sqrt\n",
        "\n",
        "# Generating synthetic regression data\n",
        "X, y = make_regression(n_samples=100, n_features=1, noise=0.1, random_state=42)\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.3, random_state=42)\n",
        "\n",
        "# Define alternative loss functions\n",
        "def quantile_loss(y_true, y_pred, tau):\n",
        "    error = y_true - y_pred\n",
        "    return np.where(error >= 0, tau * error, (tau - 1) * error)\n",
        "\n",
        "def pseudo_huber_loss(y_true, y_pred, delta):\n",
        "    error = y_true - y_pred\n",
        "    return delta**2 * (np.sqrt(1 + (error / delta)**2) - 1)\n",
        "\n",
        "def tukey_biweight_loss(y_true, y_pred, c):\n",
        "    error = np.abs(y_true - y_pred)\n",
        "    return np.where(error < c, c**2 * (1 - (1 - (error / c)**2)**3), 0.5 * c**2)\n",
        "\n",
        "# Defining function to perform KNN regression and calculate loss\n",
        "def knn_regression(k, X_train, y_train, X_test, y_test, loss_func='mse', **kwargs):\n",
        "    model = KNeighborsRegressor(n_neighbors=k)\n",
        "    model.fit(X_train, y_train)\n",
        "    y_pred = model.predict(X_test)\n",
        "\n",
        "    if loss_func == 'mse':\n",
        "        loss = np.mean((y_test - y_pred)**2)\n",
        "    elif loss_func == 'mae':\n",
        "        loss = np.mean(np.abs(y_test - y_pred))\n",
        "    elif loss_func == 'quantile':\n",
        "        loss = np.mean(quantile_loss(y_test, y_pred, kwargs['tau']))\n",
        "    elif loss_func == 'pseudo_huber':\n",
        "        loss = np.mean(pseudo_huber_loss(y_test, y_pred, kwargs['delta']))\n",
        "    elif loss_func == 'tukey_biweight':\n",
        "        loss = np.mean(tukey_biweight_loss(y_test, y_pred, kwargs['c']))\n",
        "\n",
        "    return loss\n",
        "\n",
        "# Calculating loss for different values of K and loss functions\n",
        "K_values = range(1, 21)\n",
        "losses = {'mse': [], 'mae': [], 'quantile': [], 'pseudo_huber': [], 'tukey_biweight': []}\n",
        "\n",
        "for k in K_values:\n",
        "    for loss_func in ['mse', 'mae', 'quantile', 'pseudo_huber', 'tukey_biweight']:\n",
        "        if loss_func == 'quantile':\n",
        "            tau = 0.5  # Example quantile level (can be adjusted)\n",
        "            loss = knn_regression(k, X_train, y_train, X_test, y_test, loss_func, tau=tau)\n",
        "        elif loss_func == 'pseudo_huber':\n",
        "            delta = 1.0  # Example delta value (can be adjusted)\n",
        "            loss = knn_regression(k, X_train, y_train, X_test, y_test, loss_func, delta=delta)\n",
        "        elif loss_func == 'tukey_biweight':\n",
        "            c = 1.5  # Example c value (can be adjusted)\n",
        "            loss = knn_regression(k, X_train, y_train, X_test, y_test, loss_func, c=c)\n",
        "        else:\n",
        "            loss = knn_regression(k, X_train, y_train, X_test, y_test, loss_func)\n",
        "\n",
        "        losses[loss_func].append(loss)\n",
        "\n",
        "# Plotting the results\n",
        "plt.figure(figsize=(10, 6))\n",
        "for loss_func in losses:\n",
        "    plt.plot(K_values, losses[loss_func], label=loss_func)\n",
        "plt.xlabel('K')\n",
        "plt.ylabel('Loss')\n",
        "plt.title('Loss vs. K for Different Loss Functions')\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
