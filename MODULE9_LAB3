        "import torch.optim as optim\n",
        "\n",
        "# Define hyperparameters to tune\n",
        "hyperparameters = {\n",
        "    'num_epochs': [5, 10, 15],\n",
        "    'optimizer': [optim.Adam, optim.SGD],\n",
        "    'learning_rate': [0.001, 0.01, 0.1],\n",
        "    'batch_size': [64, 128, 256]\n",
        "}\n",
        "\n",
        "# Define a function to train and evaluate the model with given hyperparameters\n",
        "def train_evaluate_model(hyperparams):\n",
        "    # Initialize the model\n",
        "    model = Net()\n",
        "    # Set optimizer and learning rate\n",
        "    optimizer = hyperparams['optimizer'](model.parameters(), lr=hyperparams['learning_rate'])\n",
        "    # Define loss function\n",
        "    criterion = nn.CrossEntropyLoss()\n",
        "    # Define dataloaders\n",
        "    trainloader = torch.utils.data.DataLoader(trainset, batch_size=hyperparams['batch_size'], shuffle=True, num_workers=2)\n",
        "    testloader = torch.utils.data.DataLoader(testset, batch_size=hyperparams['batch_size'], shuffle=False, num_workers=2)\n",
        "    # Train the model\n",
        "    train_losses, train_acc = train(hyperparams['num_epochs'], model, trainloader, criterion, optimizer)\n",
        "    # Evaluate the model on test data\n",
        "    test_accuracy = test_model(model, testloader)\n",
        "    return test_accuracy\n",
        "\n",
        "# Grid search over hyperparameters\n",
        "best_accuracy = 0\n",
        "best_hyperparams = None\n",
        "for num_epochs in hyperparameters['num_epochs']:\n",
        "    for optimizer in hyperparameters['optimizer']:\n",
        "        for learning_rate in hyperparameters['learning_rate']:\n",
        "            for batch_size in hyperparameters['batch_size']:\n",
        "                hyperparams = {\n",
        "                    'num_epochs': num_epochs,\n",
        "                    'optimizer': optimizer,\n",
        "                    'learning_rate': learning_rate,\n",
        "                    'batch_size': batch_size\n",
        "                }\n",
        "                print(f\"Evaluating model with hyperparameters: {hyperparams}\")\n",
        "                accuracy = train_evaluate_model(hyperparams)\n",
        "                print(\"Test accuracy:\", accuracy)\n",
        "\n",
        "print(\"Best hyperparameters:\", best_hyperparams)\n",
        "print(\"Best test accuracy:\", best_accuracy)\n"
